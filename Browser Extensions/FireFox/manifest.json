{
  "manifest_version": 2,
  "name": "FileFLux",
  "version": "1.0",
  "description": "Intercepts downloads and redirects them to FileFlux.",
  "permissions": [
    "downloads",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "<all_urls>"
  ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  }
}