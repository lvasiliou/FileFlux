{
  "manifest_version": 3,
  "name": "FileFLux",
  "version": "1.0",
  "description": "Intercepts downloads and redirects them to FileFlux.",
  "permissions": [
    "downloads",
    "downloads.open",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}